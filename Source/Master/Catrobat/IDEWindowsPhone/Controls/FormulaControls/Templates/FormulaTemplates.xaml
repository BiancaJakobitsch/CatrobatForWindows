  <f:FormulaDefinitionCollection x:Key="FormulaDefinitions">
    <!-- ### Unknown Formula ##########################################################################################################-->
    <f:FormulaDefinition Type="UNKNOWN">
      <f:FormulaDefinition.Template>
        <partControls:FormulaPartControlList>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="Unknown" Style="{StaticResource PartStyleMath}"/>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="OpenBrecket" Style="{StaticResource PartStyleMath}"/>
          <partControls:FormulaPartControlPlaceholderLeft/>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="ParameterSeperator" Style="{StaticResource PartStyleMath}"/>
          <partControls:FormulaPartControlPlaceholderRight/>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="ClosedBrecket" Style="{StaticResource PartStyleMath}"/>
        </partControls:FormulaPartControlList>
      </f:FormulaDefinition.Template>
    </f:FormulaDefinition>

    <!-- ### Number Formulas ##########################################################################################################-->
    <f:FormulaDefinition Type="#EMPTY#">
      <f:FormulaDefinition.Template>
        <partControls:FormulaPartControlList>
          <partControls:FormulaPartControlText Style="{StaticResource PartStyleEmpty}" />
        </partControls:FormulaPartControlList>
      </f:FormulaDefinition.Template>
    </f:FormulaDefinition>

    <!-- ### Number Formulas ##########################################################################################################-->
    <f:FormulaDefinition Type="NUMBER">
      <f:FormulaDefinition.Template>
        <partControls:FormulaPartControlList>
          <partControls:FormulaPartControlText Style="{StaticResource PartStyleNumber}" />
        </partControls:FormulaPartControlList>
      </f:FormulaDefinition.Template>
    </f:FormulaDefinition>


    <!-- ### Math Formulas ##########################################################################################################-->
    <f:FormulaDefinition Type="FUNCTION" Value="RANDOM">
      <f:FormulaDefinition.Template>
        <partControls:FormulaPartControlList>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="Random" Style="{StaticResource PartStyleMath}"/>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="OpenBrecket" Style="{StaticResource PartStyleMath}"/>
          <partControls:FormulaPartControlPlaceholderLeft/>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="ParameterSeperator" Style="{StaticResource PartStyleMath}"/>
          <partControls:FormulaPartControlPlaceholderRight/>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="ClosedBrecket" Style="{StaticResource PartStyleMath}"/>
        </partControls:FormulaPartControlList>
      </f:FormulaDefinition.Template>
    </f:FormulaDefinition>


    <!-- ### Logic Equality Formulas ##########################################################################################################-->
    <f:FormulaDefinition Type="OPERATOR" Value="EQUAL">
      <f:FormulaDefinition.Template>
        <partControls:FormulaPartControlList>
          <partControls:FormulaPartControlPlaceholderLeft/>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="LogicEqual" Style="{StaticResource PartStyleOperatorEquality}"/>
          <partControls:FormulaPartControlPlaceholderRight/>
        </partControls:FormulaPartControlList>
      </f:FormulaDefinition.Template>
    </f:FormulaDefinition>


    <!-- ### Logic Formulas ##########################################################################################################-->
    <f:FormulaDefinition Type="OPERATOR" Value="OR">
      <f:FormulaDefinition.Template>
        <partControls:FormulaPartControlList>
          <partControls:FormulaPartControlPlaceholderLeft/>
          <partControls:FormulaPartControlStaticLocalizedText LocalizedResourceName="LogicOr" Style="{StaticResource PartStyleOperatorLogic}"/>
          <partControls:FormulaPartControlPlaceholderRight/>
        </partControls:FormulaPartControlList>
      </f:FormulaDefinition.Template>
    </f:FormulaDefinition>

    <!-- ### Object Variable ##########################################################################################################-->
    <f:FormulaDefinition Type="SENSOR">
      <f:FormulaDefinition.Values>
        <f:StringList>
          <system:String>OBJECT_X</system:String>
          <system:String>OBJECT_Y</system:String>
          <system:String>TRANSPARENCY</system:String>
          <system:String>BRIGHTNESS</system:String>
          <system:String>SIZE</system:String>
          <system:String>DIRECTION</system:String>
          <system:String>LAYER</system:String>
        </f:StringList>
      </f:FormulaDefinition.Values>
      <f:FormulaDefinition.Template>
        <partControls:FormulaPartControlList>
          <partControls:FormulaPartControlDynamicLocalizedText Style="{StaticResource PartStyleObjectVariable}"/>
        </partControls:FormulaPartControlList>
      </f:FormulaDefinition.Template>
    </f:FormulaDefinition>


    <!-- ### Sensor Variable ##########################################################################################################-->
    <f:FormulaDefinition Type="SENSOR">
      <f:FormulaDefinition.Values>
        <f:StringList>
          <system:String>ACCELERATION_X</system:String>
          <system:String>ACCELERATION_Y</system:String>
          <system:String>ACCELERATION_Z</system:String>
          <system:String>COMPASSDIRECTION</system:String>
          <system:String>INCLINATION_X</system:String>
          <system:String>INCLINATION_Y</system:String>
        </f:StringList>
      </f:FormulaDefinition.Values>
      <f:FormulaDefinition.Template>
